{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "server": "nodemon dist/index.js",
    "watch": "tsc -w",
    "dev": "concurrently \"yarn run watch\" \"yarn run server\" \"yarn run watch-views\"",
    "build": "tsc && yarn run copy-files",
    "copy-files": "copyfiles -u 1 \"src/views/**/*\" dist/",
    "watch-views": "nodemon -w src/views -e ejs -x \"yarn run copy-files\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "type": "module",
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.12.10",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.14",
    "prisma": "^5.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/node": "^22.4.0",
    "@types/nodemailer": "^6.4.15",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.4",
    "typescript": "^5.5.4"
  }
}
